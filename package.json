{
  "name": "bedrock-audit",
  "version": "0.0.1",
  "description": "Audits to use on frontend",
  "repository": {
    "type": "git",
    "url": "git@bitbucket.org:sendoushi/bedrock-audit.git"
  },
  "authors": [
    "Joel Santos <joel@sendoushi.com>"
  ],
  "main": "./dist/index.js",
  "engines": {
    "node": ">=6"
  },
  "scripts": {
    "clean": "rm -rf dist/*",
    "test": "npm run eslint && npm run mocha",
    "eslint": "eslint ./src/**/*.js",
    "mocha": "BABEL_ENV=test mocha ./test/test.js --compilers js:babel-core/register",
    "prebuild": "npm run clean && npm run test",
    "build": "BABEL_ENV=production babel src --out-dir dist --source-maps inline --ignore *.spec.js,_test/**/*",
    "prepublish": "npm run test"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.20.0",
    "babel-eslint": "^6.1.2",
    "babel-plugin-remove-code": "0.0.4",
    "babel-preset-es2015": "^6.18.0",
    "es6-promise": "^4.0.5",
    "eslint": "^2.0.0",
    "eslint-config-defaults": "^9.0.0",
    "eslint-plugin-react": "^6.2.0",
    "glob": "^7.1.1",
    "lodash": "^4.17.4"
  },
  "dependencies": {
    "chai": "^3.5.0",
    "joi": "^9.2.0",
    "jsdom": "^9.8.3",
    "lighthouse": "^1.2.2",
    "mocha": "^3.2.0",
    "request": "^2.79.0",
    "w3cjs": "^0.3.0",
    "yargs": "^6.3.0"
  }
}
