{
  "name": "bedrock_audit",
  "version": "0.0.1",
  "description": "Audits to use on frontend",
  "repository": {
    "type": "git",
    "url": "git@bitbucket.org:sendoushi/bedrock-audit.git"
  },
  "scripts": {
    "test": "npm run eslint-test; npm run mocha-test",
    "mocha-test": "node ./node_modules/mocha/bin/mocha ./test/test.js",
    "eslint-test": "node ./node_modules/.bin/eslint -- ./suite/**/*.js",
    "audit-test": "export ENV_BEDROCK_AUDIT_BASE=\"http://google.pt\" && node ./node_modules/mocha/bin/mocha ./suite/audit.js --config=\"./test/examples/config.json\""
  },
  "devDependencies": {
    "babel-eslint": "^6.1.2",
    "eslint": "^2.0.0",
    "eslint-config-defaults": "^9.0.0",
    "eslint-plugin-react": "^6.2.0"
  },
  "dependencies": {
    "chai": "^3.5.0",
    "joi": "^9.2.0",
    "jsdom": "^9.8.3",
    "lighthouse": "^1.2.2",
    "mocha": "^3.2.0",
    "w3cjs": "^0.3.0",
    "yargs": "^6.3.0"
  }
}
