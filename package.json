{
  "name": "auditing",
  "version": "0.0.5",
  "description": "Set the audits once and just run the tests",
  "repository": {
    "type": "git",
    "url": "git@github.com:Sendoushi/auditing.git"
  },
  "authors": [
    "Joel Santos <joel@sendoushi.com>"
  ],
  "main": "./dist/index.js",
  "engines": {
    "node": ">=6"
  },
  "scripts": {
    "clean": "rm -rf dist/*",
    "test": "npm run eslint && npm run mocha",
    "eslint": "eslint ./src/**/*.js",
    "mocha": "BABEL_ENV=test mocha ./src/**/*.spec.js ./src/**/**/*.spec.js --compilers js:babel-core/register",
    "prebuild": "npm run clean && npm run test",
    "build": "BABEL_ENV=production babel src --out-dir dist --source-maps inline --ignore *.spec.js,_test/**/*",
    "prepublish": "npm run test",
    "audit-test": "export AUDIT_BASE=\"http://google.pt\" && mocha ./src/index.js --compilers js:babel-core/register --mocha=true --config=\"./src/_test/data/config_basic.json\""
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.20.0",
    "babel-eslint": "^6.1.2",
    "babel-plugin-remove-code": "0.0.4",
    "babel-preset-es2015": "^6.18.0",
    "es6-promise": "^4.0.5",
    "eslint": "^2.0.0",
    "eslint-config-defaults": "^9.0.0",
    "eslint-plugin-react": "^6.2.0"
  },
  "dependencies": {
    "chai": "^3.5.0",
    "joi": "^9.2.0",
    "jsdom": "^9.8.3",
    "lighthouse": "^1.2.2",
    "lodash": "^4.17.4",
    "mocha": "^3.2.0",
    "tough-cookie": "^2.3.2",
    "w3cjs": "^0.3.0",
    "yargs": "^6.3.0"
  },
  "keywords": [
    "audit",
    "tests",
    "mocha"
  ]
}
